{
  "name": "layout-preset",
  "displayName": "Layout Preset",
  "description": "Store and recall editor/panel/sidebar layouts as named presets.",
  "version": "0.0.5",
  "icon": "media/marketplace-icon.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "publisher": "Steven77nl",
  "repository": {
    "type": "git",
    "url": "https://github.com/Steven77nl/layoutpreset.git"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "sharp": "^0.34.3",
    "typescript": "^4.4.3"
  },
  "contributes": {
    "commands": [
      {
        "command": "layoutPreset.savePreset",
        "title": "Layout Preset: Save Current Preset..."
      },
      {
        "command": "layoutPreset.applyPreset",
        "title": "Layout Preset: Apply Preset..."
      },
      {
        "command": "layoutPreset.listPresets",
        "title": "Layout Preset: List Presets..."
      },
      {
        "command": "layoutPreset.deletePreset",
        "title": "Layout Preset: Delete Preset..."
      }
    ]
  }
}
